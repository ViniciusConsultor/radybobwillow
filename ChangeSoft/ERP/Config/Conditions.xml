<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  
  <Condition>
    <Name>Language</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="zh-CN">中文[zh-CN]</Item>
        <Item value="en-US">英文[en-US]</Item>
      </List>
    </Data>
    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="zh-CN">Chinese[zh-CN]</Item>
        <Item value="en-US">English[en-US]</Item>
      </List>
    </Data>
  </Condition>
  
  
  <Condition>
    <Name>ExchangeCurr</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="RMB">人民币</Item>
        <Item value="JPY">人民币/日币</Item>
        <Item value="USD">人民币/美元</Item>
      </List>
    </Data>

    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="RMB">RMB/RMB</Item>
        <Item value="JYP">RMB/US</Item>
        <Item value="USD">RMB/JPY</Item>
      </List>
    </Data>
  </Condition>


  <Condition>
    <Name>CalcMode</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="M">乘算</Item>
        <Item value="D">除算</Item>
      </List>
    </Data>

    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="M">Multiplication</Item>
        <Item value="D">Division</Item>
        
      </List>
    </Data>
  </Condition>
  
  
  
</Settings>