<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  
  <Condition>
    <Name>Language</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="zh-CN">中文[zh-CN]</Item>
        <Item value="en-US">英文[en-US]</Item>
      </List>
    </Data>
    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="zh-CN">Chinese[zh-CN]</Item>
        <Item value="en-US">English[en-US]</Item>
      </List>
    </Data>
  </Condition>
  
  
  <Condition>
    <Name>ExchangeCurr</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="RMB">人民币/RMB</Item>
        <Item value="JPY">日币/JPY</Item>
        <Item value="USD">美元/USD</Item>
      </List>
    </Data>

    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="RMB">RMB/RMB</Item>
        <Item value="JYP">JYP</Item>
        <Item value="USD">USD</Item>
      </List>
    </Data>
  </Condition>


  <Condition>
    <Name>CalcMode</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="M">乘算</Item>
        <Item value="D">除算</Item>
      </List>
    </Data>

    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="M">Multiplication</Item>
        <Item value="D">Division</Item>
        
      </List>
    </Data>
  </Condition>

    <Condition>
        <Name>ActionMode</Name>
        <Data>
            <Language>zh-CN</Language>
            <List>
                <Item value="C">添加模式</Item>
                <Item value="U">编辑模式</Item>
                <Item value="D">删除模式</Item>
                <Item value="R">参照模式</Item>
            </List>
        </Data>

        <Data>
            <Language>en-US</Language>
            <List>
                <Item value="C">Create Mode</Item>
                <Item value="U">Update Mode</Item>
                <Item value="D">Delete Mode</Item>
                <Item value="R">Retrieve Mode</Item>
            </List>
        </Data>
    </Condition>

    <Condition>
        <Name>LotMethod</Name>
        <Data>
            <Language>zh-CN</Language>
            <List>
                <Item value="1">经济订货批量</Item>
                <Item value="2">固定期间</Item>
                <Item value="3">固定批量</Item>
            </List>
        </Data>

        <Data>
            <Language>en-US</Language>
            <List>
                <Item value="1">EOQ</Item>
                <Item value="2">FPR</Item>
                <Item value="3">FOQ</Item>
            </List>
        </Data>
    </Condition>
</Settings>