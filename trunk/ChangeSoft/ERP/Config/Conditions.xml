<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  
  <Condition>
    <Name>Language</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="zh-CN">中文[zh-CN]</Item>
        <Item value="en-US">英文[en-US]</Item>
      </List>
    </Data>
    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="zh-CN">Chinese[zh-CN]</Item>
        <Item value="en-US">English[en-US]</Item>
      </List>
    </Data>
  </Condition>
  
  
  <Condition>
    <Name>ExchangeCurr</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="RMB">人民币/RMB</Item>
        <Item value="JPY">日币/JPY</Item>
        <Item value="USD">美元/USD</Item>
      </List>
    </Data>

    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="RMB">RMB/RMB</Item>
        <Item value="JYP">JYP</Item>
        <Item value="USD">USD</Item>
      </List>
    </Data>
  </Condition>


  <Condition>
    <Name>CalcMode</Name>
    <Data>
      <Language>zh-CN</Language>
      <List>
        <Item value="M">乘算</Item>
        <Item value="D">除算</Item>
      </List>
    </Data>

    <Data>
      <Language>en-US</Language>
      <List>
        <Item value="M">Multiplication</Item>
        <Item value="D">Division</Item>
        
      </List>
    </Data>
  </Condition>

    <Condition>
        <Name>EditMode</Name>
        <Data>
            <Language>zh-CN</Language>
            <List>
                <Item value="ADD">添加模式</Item>
                <Item value="EDIT">编辑模式</Item>
                <Item value="DELETE">删除模式</Item>
            </List>
        </Data>

        <Data>
            <Language>en-US</Language>
            <List>
                <Item value="ADD">Add Mode</Item>
                <Item value="EDIT">Edit Mode</Item>
                <Item value="DELETE">Delete Mode</Item>
            </List>
        </Data>
    </Condition>  
  
</Settings>